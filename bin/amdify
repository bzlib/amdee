#!/usr/bin/env node # -*- javascript -*- -p

require('coffee-script');

// is this 
var argv = require('optimist')
  .demand(['source'])
  //.boolean('w')
  .usage('Usage: amdify [-w] -source <source_module_dir> -target <target_output_dir>')
  .argv;

var amdify = require('../src/amdify');

amdify.run(argv);

