#!/usr/bin/env node # -*- javascript -*- -p

var argv = require('optimist')
    .alias('r', 'recursive')
    .alias('s', 'source')
    .alias('t', 'target')
    .demand(['source', 'target'])
    .usage('Usage: amdee -source <source_module_dir> -target <target_output_dir>')
    .argv;

var amdee = require('../lib/amdee2');

amdee.run(argv);

